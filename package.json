{
  "name": "Cartaro",
  "version": "2.1.3",
  "author": "Craig N. Caroon",
  "codename": "Unknown Depths",
  "icon": "mdi-submarine",
  "buildDate": "March 8th, 2024",
  "description": "Work Journal/Chronicler",
  "private": true,
  "scripts": {
    "build": "vue-cli-service electron:build",
    "build:clean": "rm -rf dist_electron/",
    "dev": "vue-cli-service electron:serve",
    "serve": "vue-cli-service electron:serve",
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "vue-cli-service lint",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@codemirror/commands": "^6.1.2",
    "@codemirror/lang-javascript": "^6.1.1",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.0.3",
    "@codemirror/lang-python": "^6.0.4",
    "@codemirror/language": "^6.3.0",
    "@codemirror/legacy-modes": "^6.2.0",
    "@codemirror/state": "^6.1.2",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@codemirror/view": "^6.4.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@uiw/codemirror-theme-dracula": "^4.12.4",
    "axios": "^1.1.3",
    "cm6-theme-basic-dark": "^0.2.0",
    "cm6-theme-material-dark": "^0.2.0",
    "codemirror": "^6.0.1",
    "crypto-browserify": "^3.12.0",
    "fernet": "^0.4.0",
    "highlight.js": "^11.6.0",
    "localforage": "^1.10.0",
    "markdown-it": "^14.0.0",
    "markdown-it-for-inline": "^2.0.1",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "moment": "^2.29.1",
    "mousetrap": "^1.6.5",
    "stream-browserify": "^3.0.0",
    "vue": "^2.7.16",
    "vue-router": "^3.6.5",
    "vuetify": "^2.7.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^8.0.0",
    "electron": "^28.1.1",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.19.2",
    "sass": "^1.69.7",
    "sass-loader": "^13.3.3",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-vuetify": "~2.5.1",
    "vue-template-compiler": "^2.7.16",
    "vuetify-loader": "^1.7.0"
  },
  "overrides": {
    "vue-cli-plugin-electron-builder": {
      "electron-builder": "$electron-builder"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "object-shorthand": "off",
      "vue/no-mutating-props": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
